<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here
clock x;
int x;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-17" y="0">
			<name x="-42" y="-42">Initial</name>
			<label kind="invariant" x="-27" y="17">x&lt;5</label>
		</location>
		<location id="id1" x="280" y="-102">
			<name x="238" y="-136">Location1</name>
		</location>
		<location id="id2" x="280" y="0">
			<name x="270" y="-34">Location2</name>
		</location>
		<location id="id3" x="272" y="110">
			<name x="262" y="76">Location3</name>
		</location>
		<branchpoint id="id4" x="688" y="510">
		</branchpoint>
		<branchpoint id="id5" x="144" y="0">
		</branchpoint>
		<init ref="id0"/>
		<transition>
			<source ref="id5"/>
			<target ref="id3"/>
			<label kind="assignment" x="212" y="76">x=3</label>
			<label kind="probability" x="170" y="51">30</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id2"/>
			<label kind="assignment" x="221" y="0">x=2</label>
			<label kind="probability" x="187" y="-17">20</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id1"/>
			<label kind="assignment" x="221" y="-59">x=1</label>
			<label kind="probability" x="161" y="-59">10</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id5"/>
			<label kind="guard" x="51" y="8">x&gt;=3</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
